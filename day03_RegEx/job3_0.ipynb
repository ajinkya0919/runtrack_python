    "def main():\n",
    "    # open or create a text file for appending content\n",
    "    myfile = open(\"output.txt\", \"a+\")\n",
    "\n",
    "    line = input(\"Enter data for output.txt: \")\n",
    "    myfile.write(\"\\n%s\" % line)\n",
    "\n",
    "    myfile.close()\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    main()"
