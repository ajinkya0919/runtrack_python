    "def drawrect(h,v):\n",
    "    for i in range(0, v):\n",
    "\n",
    "        if(i == 0 or i == v-1):\n",
    "            print('|' + '-'*(h-2) + '|')\n",
    "        else:\n",
    "            print('|' +' '*(h-2) + '|')"
   ]
